{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        //"eslint:recommended",
        //"plugin:react/recommended"
        "airbnb",
        "prettier",
        "plugin:node/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "prettier/prettier": "error",
      "import/newline-after-import": ["error"],
      "func-names": 0
    }
}
