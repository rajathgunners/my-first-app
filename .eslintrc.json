{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    //"eslint:recommended",
    //"plugin:react/recommended"
    "airbnb",
    "prettier"
    //"plugin:node/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaVersion": 12
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "off",
    "prettier/prettier": "error",
    "import/newline-after-import": [
      "error"
    ],
    "func-names": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}